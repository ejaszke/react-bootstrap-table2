{
  "name": "react-bootstrap-table2",
  "version": "0.0.1",
  "private": true,
  "description": "Rebuilt for react-bootstrap-table",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "gulp prod",
    "lint": "eslint ./packages --ext .js --ext .jsx --ignore-path .gitignore",
    "pretest": "yarn lint --cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "storybook": "yarn workspace react-bootstrap-table2-example storybook",
    "gh-pages:clean": "yarn workspace react-bootstrap-table2-example gh-pages:clean",
    "gh-pages:build": "yarn workspace react-bootstrap-table2-example gh-pages:build",
    "release": "yarn install && yarn build && lerna publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-bootstrap-table/react-bootstrap-table2.git"
  },
  "author": "AllenFang",
  "contributors": [
    {
      "name": "Edward Jaszke",
      "url": "https://github.com/ejaszke"
    },
    {
      "name": "Allen Fang",
      "email": "ayu780129@hotmail.com",
      "url": "https://github.com/AllenFang"
    },
    {
      "name": "Chun-MingChen",
      "email": "nick830314@gmail.com",
      "url": "https://github.com/Chun-MingChen"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-bootstrap-table/react-bootstrap-table2/issues"
  },
  "homepage": "https://github.com/react-bootstrap-table/react-bootstrap-table2#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "0.4.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-shell": "^0.8.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "lerna": "^7.4.1",
    "sass": "^1.77.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "classnames": "2.5.1",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "underscore": "^1.13.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.js",
      "packages/*/index.js",
      "!packages/react-bootstrap-table2-example/**/*.js"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFiles": [
      "<rootDir>/enzyme-setup.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table2"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  }
}
